<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% messages.forEach(message => { %>
        <li>
          <strong><%= message.user %></strong>: <%= message.text %> - <%= message.added.toLocaleString() %>
        </li>
      <% }); %>
    </ul>
    <a href="/new">Add New Message</a>
  </body>
</html>
